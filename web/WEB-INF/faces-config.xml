<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
   
    <application>
        <resource-bundle>
            <base-name>/parqueaderoweb/Bundle</base-name>
            <var>bundle</var>
        </resource-bundle>
    </application>
    <navigation-rule>
        <from-view-id>/inicioDePagina/iniciodepagina.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>listase</from-outcome>
            <to-view-id>/listase/listase.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>listade</from-outcome>
            <to-view-id>/listaDE/listade.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>listadec</from-outcome>
            <to-view-id>/listaDEC/listadec.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/listase/listase.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>homelistase</from-outcome>
            <to-view-id>/inicioDePagina/iniciodepagina.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/listaDE/listade.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>homelistade</from-outcome>
            <to-view-id>/inicioDePagina/iniciodepagina.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/listaDEC/listadec.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>homelistadec</from-outcome>
            <to-view-id>/inicioDePagina/iniciodepagina.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
